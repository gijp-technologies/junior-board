  "bugs": {
    "url": "https://github.com/gijp-technologies/junior-board/issues"
  },
  "homepage": "https://github.com/gijp-technologies/junior-board#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "mongoose": "^5.6.10",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1"
  },
  "engines": {
    "node": "10.15.1"
  }
